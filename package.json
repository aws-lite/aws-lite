{
  "name": "@aws-lite/client",
  "version": "0.23.2",
  "description": "A simple, fast, extensible AWS client",
  "homepage": "https://aws-lite.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aws-lite/aws-lite.git"
  },
  "bugs": "https://github.com/aws-lite/aws-lite/issues",
  "scripts": {
    "-----plugins----": "",
    "gen": "npm run generate-plugins",
    "generate-plugins": "node scripts/generate-plugins/index.mjs",
    "plugin": "node scripts/version-plugins.mjs",
    "publish-plugins": "node scripts/publish-plugins.mjs",
    "------other-----": "",
    "vendor": "scripts/vendor/vendor.sh",
    "-----testing----": "",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test:unit",
    "lint": "eslint --fix .",
    "test:live": "cross-env tape 'test/live/**/*-test.js' | tap-arc",
    "test:plugins": "cross-env tape 'plugins/**/test/**/*-test.mjs' | tap-arc",
    "test:precommit": "cross-env PRECOMMIT=true npm run gen && npm run lint",
    "test:types": "tsd --files 'test/types/**/*.test-d.ts' --files 'plugins/*/types/**/*.test-d.ts'",
    "test:unit": "cross-env tape 'test/unit/**/*-test.mjs' | tap-arc",
    "test": "npm run lint && npm run gen && npm run test:types && npm run test:plugins && npm run coverage"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run test:precommit",
    "pre-push": "npm run test"
  },
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "engines": {
    "node": ">=20"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "aws4": "^1.13.2"
  },
  "devDependencies": {
    "@actions/core": "^2.0.1",
    "@architect/eslint-config": "^3.0.0",
    "@aws-sdk/util-dynamodb": "^3.958.0",
    "@smithy/smithy-client": "^4.10.2",
    "@types/node": "^25.0.3",
    "adm-zip": "^0.5.16",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.2",
    "fast-xml-parser": "^5.3.3",
    "mock-tmp": "^0.0.4",
    "nyc": "^17.1.0",
    "semver": "^7.7.3",
    "simple-git-hooks": "^2.13.1",
    "tap-arc": "^1.3.2",
    "tape": "^5.9.0",
    "tsd": "^0.33.0"
  },
  "files": [
    "src"
  ],
  "workspaces": [
    "plugins/acm",
    "plugins/acm/types",
    "plugins/apigateway",
    "plugins/apigateway/types",
    "plugins/apigatewaymanagementapi",
    "plugins/apigatewaymanagementapi/types",
    "plugins/apigatewayv2",
    "plugins/apigatewayv2/types",
    "plugins/cloudformation",
    "plugins/cloudformation/types",
    "plugins/cloudfront",
    "plugins/cloudfront/types",
    "plugins/cloudwatch-logs",
    "plugins/cloudwatch-logs/types",
    "plugins/dynamodb",
    "plugins/dynamodb/types",
    "plugins/iam",
    "plugins/iam/types",
    "plugins/lambda",
    "plugins/lambda/types",
    "plugins/organizations",
    "plugins/organizations/types",
    "plugins/rds-data",
    "plugins/rds-data/types",
    "plugins/route53",
    "plugins/route53/types",
    "plugins/s3",
    "plugins/s3/types",
    "plugins/sns",
    "plugins/sns/types",
    "plugins/sqs",
    "plugins/sqs/types",
    "plugins/ssm",
    "plugins/ssm/types",
    "plugins/sts",
    "plugins/sts/types"
  ],
  "eslintConfig": {
    "extends": "@architect/eslint-config",
    "rules": {
      "global-require": "off"
    },
    "parserOptions": {
      "ecmaVersion": 13
    }
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "lines": 100,
    "functions": 100,
    "statements": 100,
    "exclude": [
      "plugins/",
      "test/",
      "src/_vendor/"
    ]
  }
}
